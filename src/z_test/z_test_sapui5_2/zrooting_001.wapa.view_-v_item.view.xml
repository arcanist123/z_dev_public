<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml" controllerName="ZROUTING_001.controller.V_Item">                                                                                        
	<App>                                                                                                                                                                                                                                                         
		<pages>                                                                                                                                                                                                                                                      
		    <Page title="PO Item Detail" showNavButton="true" navButtonPress="GoToFirstView">                                                                                                                                                                        
 		        <content>                                                                                                                                                                                                                                           
				    <Table noDataText="Drop column list items here and columns in the area above" id="it_items" items="{path:'/SFLIGHTSet'}">                                                                                                                              
				&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                                                                           
				        <columns>                                                                                                                                                                                                                                          
&nbsp;                                                                                                                                                                                                                                                         
                             <Column> <Text text="Connid"/> </Column>                                                                                                                                                                                          
                             <Column> <Text text="Fldate"/> </Column>                                                                                                                                                                                          
                             <Column> <Text text="Carrid"/>  </Column>                                                                                                                                                                                         
                             <Column> <Text text="Price"/>  </Column>                                                                                                                                                                                          
                             <Column> <Text text="Currency"/>  </Column>                                                                                                                                                                                       
                             <Column> <Text text="Planetype"/>  </Column>                                                                                                                                                                                      
&nbsp;                                                                                                                                                                                                                                                         
                          </columns>                                                                                                                                                                                                                           
&nbsp;                                                                                                                                                                                                                                                         
                          <items>                                                                                                                                                                                                                              
                               <ColumnListItem type="Active" press="onItemPress">                                                                                                                                                                              
                                     <Text text="{Connid}"/>                                                                                                                                                                                                   
                                     <Text text="{Fldate}"/>                                                                                                                                                                                                   
                                     <Text text="{Carrid}"/>                                                                                                                                                                                                   
                                     <Text text="{Price}"/>                                                                                                                                                                                                    
                                     <Text text="{Currency}"/>&nbsp;&nbsp;                                                                                                                                                                                     
                                     <Text text="{Planetype}"/>&nbsp;                                                                                                                                                                                          
                                </ColumnListItem>                                                                                                                                                                                                              
&nbsp;                                                                                                                                                                                                                                                         
                           </items>                                                                                                                                                                                                                            
&nbsp;                                                                                                                                                                                                                                                         
				        </Table>                                                                                                                                                                                                                                           
				</content>                                                                                                                                                                                                                                                 
			</Page>                                                                                                                                                                                                                                                     
		</pages>                                                                                                                                                                                                                                                     
	</App>                                                                                                                                                                                                                                                        
</mvc:View>                                                                                                                                                                                                                                                    